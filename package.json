{
  "name": "react-responsive-ui",
  "version": "0.15.31",
  "description": "Responsive React UI components",
  "main": "index.common.js",
  "module": "index.js",
  "peerDependencies": {
    "react": ">=0.16.8",
    "react-dom": ">=0.16.8"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-context": "^0.2.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react-create-ref": "^1.0.1",
    "react-day-picker": "^7.4.10",
    "react-lifecycles-compat": "^3.0.2",
    "react-modal": "^3.8.1",
    "request-animation-frame-timeout": "^1.0.0",
    "scroll-into-view-if-needed": "^2.2.16"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "cldr-core": "^41.0.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.1.4",
    "fs-extra": "^9.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.0.5",
    "node-sass": "^5.0.0",
    "npm-run-all": "^1.4.0",
    "postcss": "^6.0.22",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router": "^3.0.0",
    "rimraf": "^2.5.0",
    "simple-http-file-server": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "update-cldr-data": "npm install cldr-core@latest --save",
    "generate-first-day-of-week-data": "node --require babel-core/register bin/generateGetFirstDayOfWeek.js",
    "test": "mocha --require babel-core/register --require test/setup.js --colors --bail --reporter spec \"./{,!(node_modules)/**/}*.test.js\" --recursive",
    "test-coverage": "istanbul cover -x \"commonjs/**\" node_modules/mocha/bin/_mocha -- --require babel-core/register --colors --reporter dot test/ --recursive",
    "test-travis": "istanbul cover -x \"commonjs/**\" node_modules/mocha/bin/_mocha --report lcovonly -- --require babel-core/register --colors --reporter spec test/ --recursive",
    "browser-build": "webpack --mode production --progress --colors",
    "browser-build-dev": "webpack --mode development --progress --colors --watch",
    "clean-for-build": "rimraf ./commonjs/**/* ./modules/**/*",
    "build-commonjs-modules": "cross-env BABEL_ENV=commonjs babel ./source --out-dir ./commonjs --source-maps --ignore *.test.js",
    "build-es6-modules": "cross-env BABEL_ENV=es6 babel ./source --out-dir ./modules --source-maps --ignore *.test.js",
    "build-styles": "node-sass style.scss style.css && babel-node generate-demo-styles",
    "copy-styles": "copyfiles --flat source/*.css styles",
    "build": "npm-run-all update-cldr-data generate-first-day-of-week-data clean-for-build build-commonjs-modules build-es6-modules build-styles browser-build",
    "prepublish": "npm-run-all build test copy-styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/catamphetamine/react-responsive-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "responsive",
    "material",
    "design"
  ],
  "author": "catamphetamine <purecatamphetamine@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/catamphetamine/react-responsive-ui/issues"
  },
  "homepage": "https://gitlab.com/catamphetamine/react-responsive-ui#readme"
}
